  url      = {paper-resources/Articles/castells2019.pdf},
  pdf      = {paper-resources/Articles/castells2019.pdf},
  bibtex_show = true,
}

@InProceedings{ramis2018aprendizaje,
  author    = {Ramis, Jaime and Carbajo, Jesús and González, Juan de Dios and Poveda, Pedro and Requena-Plens, José M. and Segovia, Enrique G. and others},
  title     = {Aprendizaje basado en proyectos en las materias transductores ac{\'u}sticos y vibroac{\'u}stica},
